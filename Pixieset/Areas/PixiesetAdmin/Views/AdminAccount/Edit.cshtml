@model AppUser
<div class="m-3 mt-5">
    <form method="post" enctype="multipart/form-data" class="mt-4">
        <div>
            <input asp-for="FirstName" class="form-control" > <br/>
            <span class="text-danger" asp-validation-for="FirstName"></span>
        </div>
        <div>
            <input asp-for="LastName"  class="form-control" > <br/>
            <span class="text-danger" asp-validation-for="LastName"></span>
        </div>
          <div class="d-flex">
             
           <h4>Block:</h4><input asp-for="IsBlock" style="width:25px;height:25px;" class="mx-4" >
            <span class="text-danger" asp-validation-for="IsBlock"></span>
        </div>
          <div class="d-flex">
             
           <h4>Admin:</h4><input asp-for="IsAdmin" style="width:25px;height:25px;" class="mx-4" >
            <span class="text-danger" asp-validation-for="IsAdmin"></span>
        </div>
        <div>
            <button type="submit" class="btn btn-outline-dark w-25 mt-3">Edit</button>
        </div>
    </form>
</div>

